class Solution {
public:

     bool ch(int x){

        if(x==0)return false;


        while(x!=0){
            int rem=x%10;

            if(rem==0)return false;
            x=x/10;
        }

        return true;
     }

    vector<int> getNoZeroIntegers(int n) {
        

        for(int i=1;i<=n;i++){
            if(ch(i)==true && ch(n-i))return {i,n-i};
        }

        return {-1,-1};


        
    }
};
