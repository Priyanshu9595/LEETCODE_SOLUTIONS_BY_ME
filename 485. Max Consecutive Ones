class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {

        int n=nums.size();
        int ans=0;

        for(int i=0;i<n;){
            if(nums[i]!=1)i++;
            else{
                int c=0;
                int p=1;
                while(i<n && nums[i]==p){
                    i++;
                    c++;
                }

                ans=max(ans,c);
            }
        }

        return ans;
        
    }
};
